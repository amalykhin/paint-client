<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.162-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.amalykhin.paint.controllers.PictureListController">
   <children>
      <ListView fx:id="pictures" maxHeight="400.0" prefHeight="200.0" prefWidth="200.0" />
      <HBox spacing="5.0">
         <children>
            <Button fx:id="editButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleEditButtonPress" text="Edit" HBox.hgrow="ALWAYS" />
            <Button fx:id="newButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleNewButtonPress" text="New" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
</VBox>
